	<parameter name="MA_SC" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT NOW() as `now`, gsc.MA_SC, lsc.TENLOAI, sc.TEN_SC, NGAY, GIO, kp.TEN_PHONG, nn.DIEN_GIAI, hq.TEN_HQ, nv.HOTEN, GHICHU 
                    FROM GHI_SC `gsc`
                        join LOAI_SC `lsc` on lsc.LOAI_SC = gsc.LOAI_SC
                        join SU_CO `sc` on sc.MA_SC = gsc.TEN_SC
                        join KHOA_PHONG `kp` on kp.ID_PHONG = gsc.DIADIEM
                        join NGUYEN_NHAN `nn` on nn.ID_NN = gsc.LYDO
                        join HAU_QUA `hq` on hq.ID_HQ = gsc.HAUQUA
                        join NHAN_VIEN `nv` on nv.MANV = gsc.MANV
                    WHERE gsc.MA_SC = $P{MA_SC}]]>
	</queryString>
